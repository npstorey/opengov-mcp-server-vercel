{
  "build": {
    "installCommand": "pnpm install --no-frozen-lockfile",
    "buildCommand": "echo \"skip build\"",
    "outputDirectory": "."
  },
  "rewrites": [
    { "source": "/(.*)", "destination": "/api/server.ts" }
  ],
  "functions": {
    "api/server.ts": {
      "maxDuration": 60,
      "memory": 1024
    }
  }
}
